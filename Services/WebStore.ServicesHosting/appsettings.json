{
  "Logging": {
    "Debug": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System.Net.HttpClient.WebStoreAPI": "Information"
      },
        "Console": {
          "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information",
            "System.Net.HttpClient.WebStoreAPI": "Information"
          },
          "FormatterOptions": {
            "IncludeScopes": true,
            "TimestampFormat": "HH:mm:ss.ffff\t",
            "UseUtcTimestamp": true
          }
        }
      }
    },

   "Database": "SqlServer",
    "ConnectionStrings": {
      "SqlServer": "Server=(localdb)\\MSSQLLocalDB;Database=WebStore;Trusted_Connection=True;MultipleActiveResultSets=true",
      "Sqlite": "Data Source=WebStore.db"
    }
   }
